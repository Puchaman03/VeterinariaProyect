@model List<Models.VETE.Dueños>

@{
    ViewData["Title"] = "Dueños";
}

<h1>Dueños</h1>
<div class = "container"></div>
    <h6>Lista de Dueños</h6>

    <table class=" table table-striped table-sm">
        <thead>
            <tr>
            <th scope="col" style="text-align:center;">@Html.DisplayNameFor(model => Model.ElementAT(0).IdDueño))</th>
            <th scope="col" style="text-align:center;">@Html.DisplayNameFor(model => Model.ElementAT(0).Nombre))</th>
            <th scope="col" style="text-align:center;">@Html.DisplayNameFor(model => Model.ElementAT(0).Apellido))</th>
            <th scope="col" style="text-align:center;">@Html.DisplayNameFor(model => Model.ElementAT(0).Direccion))</th>
            <th scope="col" style="text-align:center;">@Html.DisplayNameFor(model => Model.ElementAT(0).Telefono))</th>
            </tr>
        </thead>
        <tbody>
           @foreach (var Dueños in Model)
           {
               <tr>
                <th scope="row" style="text-align:center;"@Dueños.IdDueño> </th>
                <td>@Dueños.Nombre</td>
                <td>@Dueños.Apellido</td>
                <td>@Dueños.Direccion</td>
                <td>@Dueños.Telefono</td>
                
               </tr>

           }
        </tbody>

    </table>
